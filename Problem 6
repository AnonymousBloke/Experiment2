#include <iostream>

using namespace std;

int main()
{
        int x, y, z, c;
        x = 0;
        y = 1;
        z = x + y;
        cout << x << ", " << y;

        for (c == 1; c <= 21; c++)
        {
                z = x + y;
                x = y;
                y = z;
                cout << ", "<< z;

                if (z == 10946)
                break;
        }
        cout << "." << endl;

        return 0;
}
