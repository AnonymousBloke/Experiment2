#include <iostream>
#include <cmath>

using namespace std;

int main()
{ 
int x;
double w, y, z, v1, v2, v3, v4, v5;


cout << "Enter First Number: "; cin >> x;
cout << "Enter Second Number: "; cin >> y;

z = 2.5;
v1 = x * y * z;
v2 = x + (y / z);
w = x - y;
v3 = abs(w / z);
v4 = x - sqrt((y+z));
v5 = x + y + z;


switch (x)
{
	case 1:
		if (y > 1 && y < 5)
		cout << "The Value of V is: " << v1 << endl;
		else if (y >= 5)
		cout << "The Value of V is: " << v2 << endl;
		else
		cout << "The Value of V is: " << v5 << endl;
		break;
	case 2:
		if (y <= 5)
		cout << "The Value of V is: " << v3 << endl;
		else if (y > 5)
		cout << "The Value of V is: " << v4 << endl;
		else
		cout << "The Value of V is: " << v5 << endl;
		break;
	default:
		cout << "The Value of V is: " << v5 << endl;
	
}
	return 0;
}
